function isIntArray(arr) {
  if (!arr)
    return false;
    
  for (var i = 0; i < arr.length; i++) {
    if (typeof arr[i] !== "number" || !(arr[i]) || arr[i] % 1 !== 0)
      return false;
  }
  return true;
}





function isInArray(arr) {
	return Array.isArray(arr) && arr.every(function(x) {return Math.floor === x});
}